    <LDBNAME>D$S</LDBNAME>
   </PROGDIR>
   <TPOOL>
    <item>
     <ID>S</ID>
     <KEY>P_FILE</KEY>
     <ENTRY>Initial file name</ENTRY>
     <LENGTH>25</LENGTH>
    </item>
    <item>
     <ID>S</ID>
     <KEY>P_TESTRN</KEY>
     <ENTRY>Testrun</ENTRY>
     <LENGTH>15</LENGTH>
    </item>
    <item>
